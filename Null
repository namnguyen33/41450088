local spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Entity%20Spawner/V2/Source.lua"))()
local entity = spawner.Create({
    Entity = {
        Name = "WAIL",
        Asset = "https://github.com/DoorsHubs/Doors-Hubs/raw/refs/heads/main/3.rbxm",
        HeightOffset = 0    },
    Lights = {
        Flicker = {
        Enabled = true,
        Duration = 1,
	},
        Shatter = true,
        Repair =  false,
    },
    Earthquake = {
        Enabled = true,
    },
    CameraShake = {
        Enabled = true,
        Range = 100,
        Values = {1.5, 20, 0.1, 1}
    },
    Movement = {
        Speed = 300,
        Delay = 2,
        Reversed = false,
    },
    Rebounding = {
        Enabled = true,
        Type = "Ambush",
        Min = 1,
        Max = 1,
        Delay = 2,
    },
    Damage = {
        Enabled = true,
        Range = 40,
        Amount = 125,
    },
    Crucifixion = {
        Enabled = true,
        Range = 40,
        Resist = false,
        Break = true,
    },
    Death = {
        Type = "Guiding",
        Hints = {"Death", "Hints", "Go", "Here"},
        Cause = " ",
    }
})

