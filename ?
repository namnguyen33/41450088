task.delay(5, function()
    local success, Crucifix = pcall(function()
        return require(game.ReplicatedStorage.ClientModules.EntityModules.Crucifix)
    end)

    if success and Crucifix and Crucifix.BlockedEntities then
        local entitiesToSupport = {
            "WH1T3", "SPECTRA-X", "NIGHTMARE DREAD", "Shocker", "Detph",
            "Overseer", "Trauma", "Depth 2", "Twisted Gold", "Smile",
            "Matcher", "Blink", "Threat", "Hunger", "Rebound",
            "Rebound 2", "NULL", "A200", "A60", "Darkness",
            "PURPLE WH1T3", "SMILE", "THREAT", "BLINK"
        }

        for _, name in ipairs(entitiesToSupport) do
            Crucifix.BlockedEntities[name] = true
        end
    else
        warn("Failed to load Crucifix module or Crucifix.BlockedEntities is missing")
    end
end)
